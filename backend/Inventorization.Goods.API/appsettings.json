{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "GoodsDatabase": "Host=localhost;Port=5433;Database=inventorization_goods;Username=postgres;Password=postgres"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-key-change-in-production-min-32-chars",
    "Issuer": "Inventorization.Auth",
    "Audience": "Inventorization.Client",
    "ExpirationMinutes": 60
  },
  "Projection": {
    "DynamicResultType": "ExpandoObject",
    "CacheExpressions": true,
    "MaxCachedExpressions": 100
  }
}
